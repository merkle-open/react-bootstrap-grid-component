  "private": false,
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
	"clean": "npx rimraf package-lock.json node_modules",
    "prepublishOnly": "npm-run-all prepublishOnly:*",
    "prepublishOnly:clean": "rimraf dist",
    "prepublishOnly:typescript": "tsc -p tsconfig.prod.json",
    "prepublishOnly:scss": "copyfiles -f src/components/sizingbreakpoints.scss dist/",
    "prepublishOnly:pkg-ok": "pkg-ok",
    "prettier": "prettier --write \"**/*.*(js|jsx|ts|tsx|json)\"",
	"release": "standard-version",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "update-dependencies": "npx npm-check-updates -u"
  },
  "peerDependencies": {
    "react": ">= 16 < 17"
  },
  "devDependencies": {
    "@namics/prettier-config": "0.2.0",
    "@namics/ts-config": "0.2.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "@types/react": "^16.4.17",
    "@types/react-dom": "^16.0.9",
    "@storybook/react": "4.1.18",
    "@storybook/addon-knobs": "4.1.18",
    "@types/storybook__react": "4.0.2",
    "@types/jest": "^23.3.5",
    "@types/node": "^10.12.0",
    "bootstrap": "4.3.1",
    "copyfiles": "2.1.0",
    "node-sass": "^4.9.4",
    "npm-run-all": "4.1.5",
    "pkg-ok": "2.3.1",
    "prettier": "1.16.3",
    "rimraf": "2.6.3",
    "sass-loader": "7.1.0",
    "standard-version": "4.4.0",
    "storybook-addon-jsx": "6.0.0",
    "storybook-readme": "4.0.5",
    "typescript": "3.2.4",
    "jquery": "3.4.1",
    "popper.js": "1.15.0"
  }
}
